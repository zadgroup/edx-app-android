<?xml version='1.0' encoding='UTF-8'?>
<resources>
  <!--An error occurred but we don't know why-->
  <string name="error_unknown">Eroare necunoscută. Te rugăm să reîncerci mai târziu.</string>
  <!--Network error messages-->
  <!--Error shown when  user attempts an action but needs an internet connection-->
  <string name="network_not_connected">Nu ești conectat la internet.</string>
  <!--Concise error shown when user attempts an action but needs an internet connection-->
  <string name="network_not_connected_short">Rețeaua nu poate fi găsită</string>
  <!--Alert dialog title shown when a network request fails-->
  <string name="reset_no_network_title">Eroare de conexiune</string>
  <!--Alert dialog content shown when a network request fails because the user has no internet connection-->
  <string name="reset_no_network_message">Nu ești conectat la internet. Te rugăm să verifici conexiunea la internet.</string>
  <!--An error ocurred due to some kind of network failure-->
  <string name="network_connected_error">Eroare la conectare. Te rugăm să verifici conexiunea la internet.</string>
  <!--Title of prompt indicating issue loading a video-->
  <string name="network_error_header">Probleme de încărcare a clipului video</string>
  <!--Content of prompt indicating issue loading a video-->
  <string name="network_error_message">Te rugăm să verifici conexiunea la internet.</string>
  <!--The server we are trying to use is not available right now-->
  <string name="network_service_unavailable">Serviciul nu este disponibil. Te rugăm să reîncerci mai târziu.</string>
  <!--Login error messages-->
  <!--Error shown during login when user attempts login without entering a username or e-mail-->
  <string name="error_enter_email">Introdu numele tău de utilizator sau adresa e-mail și încearcă din nou.</string>
  <!--Error shown during login when user enters an invalid email address-->
  <string name="error_invalid_email">Asigură-te că adresa e-mail e în format corect și încearcă din nou.</string>
  <!--Error shown during login when user attempts login without entering a password-->
  <string name="error_enter_password">Reintrodu parola și încearcă din nou.</string>
  <!--Title of alert shown during login when user attempts login via Facebook without having linked their account-->
  <string name="error_account_not_linked_title_fb">Contul de Facebook nu este asociat cu contul {platform_name}</string>
  <!--Content of alert shown during login when user attempts login via Facebook without having linked their account-->
  <string name="error_account_not_linked_desc_fb">Asigură-te că ai contul de Facebook asociat cu contul {platform_name} de pe {platform_destination}</string>
  <!--Title of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_title_google">Contul de Google nu este asociat cu contul {platform_name}</string>
  <!--Content of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_desc_google">Asigură-te că ai contul de Google asociat cu contul {platform_name} de pe {platform_destination}</string>
  <!--Download error messages-->
  <!--Label shown on video list item whose download failed-->
  <string name="error_download_failed">Descărcare eșuată</string>
  <!--Confirmation message shown when attempting to download a large quantity of video data-->
  <string name="file_size_exceeded">Mărimea descărcării depășește spațiul liber rămas pe dispozitiv.</string>
  <!--Confirmation message shown when attempting to download a large quantity of video data-->
  <string name="download_exceed_message">Videoclipurile pe care le-ai selectat sunt mai mari de 1 GB. 
Dorești să descarci aceste videoclipuri?</string>
  <!--Message shown when user attempts to download a video over cellular data,
    but has chosen to only allow video downloads over wifi-->
  <string name="wifi_off_message">Setările curente de descărcare permit descărcarea doar prin conexiunea Wi-Fi.
\n Te rugăm să te conectezi la o rețea Wi-Fi sau să modifici setările curente de descărcare.</string>
  <!--Offline Access-->
  <!--Label shown when user attempts to view a course section that hasn't been synced-->
  <string name="section_offline_header">Această secțiune nu este disponibilă în modul deconectat.</string>
  <!--Label shown when user attempts to view a course section that hasn't been synced-->
  <string name="section_offline_message">Dacă ai descărcat conținut pentru o altă secțiune, 
 \nmergi la secțiunea respectivă pentru a vedea conținutul.</string>
  <!--Alert dialog title shown when user attempts to view a video that hasn't been synced-->
  <string name="video_offline_header">Clipul video nu este disponibil în modul deconectat.</string>
  <!--Alert dialog content shown when user attempts to view a video that hasn't been synced-->
  <string name="video_offline_message">Selectează un videoclip pe care l-ai descărcat pe dispozitivul tău.</string>
  <!--New version available-->
  <!--Snackbar notification shown when the current version of the app is unsupported-->
  <string name="app_version_unsupported">Versiunea ta a acestei aplicații nu mai este acceptată.</string>
  <!--Snackbar notification shown when the current version of the app is deprecated-->
  <!--Snackbar notification shown when the current version of the app is outdated-->
  <string name="app_version_outdated">O nouă versiune a acestei aplicații este disponibilă.</string>
  <!--Snackbar action button label for opening an app store to upgrade the app to the latest version-->
  <string name="app_version_update_button">Actualizează</string>
  <!--Toast notification for when the user presses the update action button from the Snackbar
         notifications while no supported app store or browser is installed on the device with which
         the app could be updated-->
  <string name="app_version_upgrade_app_store_unavailable">Nu s-a găsit nici un magazin cu această aplicație pe dispozitiv</string>
  <!--Registration Error Messages-->
  <!--Error prompt shown during registration to indicate an empty field. Argument is a field name-->
  <string name="error_enter_field">Te rugăm să introduci %1$s.</string>
  <!--Error prompt shown during registration to indicate a field missing a selection. Argument is a field name-->
  <string name="error_select_field">Te rugăm să selectezi %1$s.</string>
  <!--Error prompt shown during registration when a field entry is too short. Arguments are 1) a field name, 2) a number-->
  <string name="error_min_length">%1$s trebuie să conțină cel puțin %2$d caractere.</string>
  <!--Error prompt shown during registration when a field entry is too long. Arguments are 1) a field name, 2) a number-->
  <string name="error_max_length">%1$s nu poate conține mai mult de %2$d caractere.</string>
  <!--Error message shown when a course hasn't started yet-->
  <string name="course_not_started">Acest curs încă nu a început .</string>
  <!--Error message shown when a user can't perform an action on e.g. add a post/response/comment in forums-->
  <string name="action_not_completed">Acțiunea ta nu a putut fi finalizată.</string>
</resources>
