<?xml version='1.0' encoding='UTF-8'?>
<resources>
  <!--An error occurred but we don't know why-->
  <string name="error_unknown">Error desconegut. Torna-ho a provar més tard.</string>
  <!--Network error messages-->
  <!--Error shown when  user attempts an action but needs an internet connection-->
  <string name="network_not_connected">No estàs connectat a internet.</string>
  <!--Concise error shown when user attempts an action but needs an internet connection-->
  <string name="network_not_connected_short">No es pot accedir a la xarxa</string>
  <!--Alert dialog title shown when a network request fails-->
  <string name="reset_no_network_title">Error en la connexió</string>
  <!--Alert dialog content shown when a network request fails because the user has no internet connection-->
  <string name="reset_no_network_message">No estàs connectat a internet. Comprova la teva connexió.</string>
  <!--An error ocurred due to some kind of network failure-->
  <string name="network_connected_error">Error en la connexió. Comprova la teva connexió a internet.</string>
  <!--Title of prompt indicating issue loading a video-->
  <string name="network_error_header">Problema al carregar el vídeo</string>
  <!--Content of prompt indicating issue loading a video-->
  <string name="network_error_message">Comprova la teva connexió a internet.</string>
  <!--The server we are trying to use is not available right now-->
  <string name="network_service_unavailable">El servei no està disponible. Torna-ho a provar més tard.</string>
  <!--Login error messages-->
  <!--Error shown during login when user attempts login without entering a username or e-mail-->
  <string name="error_enter_email">Prova d\'introduir el teu nom d\'usuari o correu electrònic un altre cop.</string>
  <!--Error shown during login when user enters an invalid email address-->
  <string name="error_invalid_email">Assegura\'t que el teu correu electrònic està ben escrit i prova un altre cop.</string>
  <!--Error shown during login when user attempts login without entering a password-->
  <string name="error_enter_password">Prova d\'introduir la teva contrasenya un altre cop.</string>
  <!--Title of alert shown during login when user attempts login via Facebook without having linked their account-->
  <string name="error_account_not_linked_title_fb">La conta de Facebook no està vinculada amb la conta de {platform_name}</string>
  <!--Content of alert shown during login when user attempts login via Facebook without having linked their account-->
  <string name="error_account_not_linked_desc_fb">Assegura\'t que la teva conta de Facebook està vinculada amb la teva conta de {platform_name} a {platform_destination}</string>
  <!--Title of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_title_google">La conta de Google no està vinculada amb la conta de {platform_name}</string>
  <!--Content of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_desc_google">Assegura\'t que la teva conta de Google està vinculada amb la teva conta de {platform_name} a {platform_destination}</string>
  <!--Download error messages-->
  <!--Label shown on video list item whose download failed-->
  <string name="error_download_failed">Error en la baixada</string>
  <!--Confirmation message shown when attempting to download a large quantity of video data-->
  <string name="file_size_exceeded">No hi ha prou espai al dispositiu.</string>
  <!--Confirmation message shown when attempting to download a large quantity of video data-->
  <string name="download_exceed_message">Els vídeos seleccionats són més grans d\'1GB.
Vols baixar els vídeos?</string>
  <!--Message shown when user attempts to download a video over cellular data,
    but has chosen to only allow video downloads over wifi-->
  <string name="wifi_off_message">La teva configuració de baixades només permet baixades mitjançant wi-fi.
\n Connecta\'t a una xarxa wi-fi o canvia la configuració de baixades.</string>
  <!--Offline Access-->
  <!--Label shown when user attempts to view a course section that hasn't been synced-->
  <string name="section_offline_header">Aquesta secció no està disponible en el mode sense connexió</string>
  <!--Label shown when user attempts to view a course section that hasn't been synced-->
  <string name="section_offline_message">Si has baixat contingut d\'una secció diferent,
\nves a la secció per veure el contingut.</string>
  <!--Alert dialog title shown when user attempts to view a video that hasn't been synced-->
  <string name="video_offline_header">Aquest vídeo no està disponible en el mode sense connexió</string>
  <!--Alert dialog content shown when user attempts to view a video that hasn't been synced-->
  <string name="video_offline_message">Selecciona un vídeo que hagis baixat al teu dispositiu.</string>
  <!--New version available-->
  <!--Snackbar notification shown when the current version of the app is unsupported-->
  <string name="app_version_unsupported">La versió de l\'aplicació ja no és compatible.</string>
  <!--Snackbar notification shown when the current version of the app is deprecated-->
  <!--Snackbar notification shown when the current version of the app is outdated-->
  <string name="app_version_outdated">Hi ha una nova versió de l\'aplicació disponible.</string>
  <!--Snackbar action button label for opening an app store to upgrade the app to the latest version-->
  <string name="app_version_update_button">Actualitza</string>
  <!--Toast notification for when the user presses the update action button from the Snackbar
         notifications while no supported app store or browser is installed on the device with which
         the app could be updated-->
  <string name="app_version_upgrade_app_store_unavailable">No supported app store found on device.</string>
  <!--Registration Error Messages-->
  <!--Error prompt shown during registration to indicate an empty field. Argument is a field name-->
  <string name="error_enter_field">Introduir %1$s.</string>
  <!--Error prompt shown during registration to indicate a field missing a selection. Argument is a field name-->
  <string name="error_select_field">Seleccionar %1$s.</string>
  <!--Error prompt shown during registration when a field entry is too short. Arguments are 1) a field name, 2) a number-->
  <string name="error_min_length">%1$s ha de tenir com a mínim %2$d caràcters.</string>
  <!--Error prompt shown during registration when a field entry is too long. Arguments are 1) a field name, 2) a number-->
  <string name="error_max_length">%1$s no pot tenir més de %2$d caràcters.</string>
  <!--Error message shown when a course hasn't started yet-->
  <string name="course_not_started">Aquest curs encara no ha començat.</string>
  <!--Error message shown when a user can't perform an action on e.g. add a post/response/comment in forums-->
  <string name="action_not_completed">No s\'ha completat l\'acció.</string>
</resources>
