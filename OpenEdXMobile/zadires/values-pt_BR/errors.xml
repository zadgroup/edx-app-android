<?xml version='1.0' encoding='UTF-8'?>
<resources>
  <!--An error occurred but we don't know why-->
  <string name="error_unknown">Erro desconhecido. Tente novamente mais tarde.</string>
  <!--Network error messages-->
  <!--Error shown when  user attempts an action but needs an internet connection-->
  <string name="network_not_connected">Você não está conectado à Internet.</string>
  <!--Concise error shown when user attempts an action but needs an internet connection-->
  <string name="network_not_connected_short">Rede indisponível</string>
  <!--Alert dialog title shown when a network request fails-->
  <string name="reset_no_network_title">Erro de Conexão.</string>
  <!--Alert dialog content shown when a network request fails because the user has no internet connection-->
  <string name="reset_no_network_message">Você não está conectado à Internet. Verifique sua conexão de Internet.</string>
  <!--An error ocurred due to some kind of network failure-->
  <string name="network_connected_error">Erro de conexão. Verifique sua conexão de Internet.</string>
  <!--Title of prompt indicating issue loading a video-->
  <string name="network_error_header">Problema ao carregar o vídeo.</string>
  <!--Content of prompt indicating issue loading a video-->
  <string name="network_error_message">Por favor, verifique sua conexão com a Internet.</string>
  <!--The server we are trying to use is not available right now-->
  <string name="network_service_unavailable">O serviço está indisponível. Tente novamente mais tarde.</string>
  <!--Login error messages-->
  <!--Error shown during login when user attempts login without entering a username or e-mail-->
  <string name="error_enter_email">Insira seu nome de usuário ou endereço de e-mail e tente novamente.</string>
  <!--Error shown during login when user enters an invalid email address-->
  <string name="error_invalid_email">Certifique-se de que seu endereço de e-mail está no formato correto e tente novamente.</string>
  <!--Error shown during login when user attempts login without entering a password-->
  <string name="error_enter_password">Insira a sua senha e tente novamente.</string>
  <!--Title of alert shown during login when user attempts login via Facebook without having linked their account-->
  <string name="error_account_not_linked_title_fb">A sua conta do Facebook não está associada à conta do {platform_name}.</string>
  <!--Content of alert shown during login when user attempts login via Facebook without having linked their account-->
  <string name="error_account_not_linked_desc_fb">Certifique-se que a sua conta está associada à conta do {platform_name} em {platform_destination}.</string>
  <!--Title of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_title_google">A sua conta no Google não está associada à conta do {platform_name}.</string>
  <!--Content of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_desc_google">Certifique-se de que a sua conta Google está associada à conta do {platform_name} em {platform_destination}.</string>
  <!--Download error messages-->
  <!--Label shown on video list item whose download failed-->
  <string name="error_download_failed">Falha no download</string>
  <!--Confirmation message shown when attempting to download a large quantity of video data-->
  <string name="file_size_exceeded">O tamanho do download excede o espaço de armazenagem restante no dispositivo.</string>
  <!--Confirmation message shown when attempting to download a large quantity of video data-->
  <string name="download_exceed_message">Os vídeos que você selecionou são maiores que 1GB. 
Deseja baixá-los?</string>
  <!--Message shown when user attempts to download a video over cellular data,
    but has chosen to only allow video downloads over wifi-->
  <string name="wifi_off_message">Suas configurações atuais permitem apenas downloads via Wi-Fi.
\n Conecte-se a uma rede Wi-Fi ou altere suas configurações de download.</string>
  <!--Offline Access-->
  <!--Label shown when user attempts to view a course section that hasn't been synced-->
  <string name="section_offline_header">Esta seção não está disponível em modo offline.</string>
  <!--Label shown when user attempts to view a course section that hasn't been synced-->
  <string name="section_offline_message">Se você baixar um conteúdo para uma seção diferente, 
\n vá para a seção para visualizá-lo.</string>
  <!--Alert dialog title shown when user attempts to view a video that hasn't been synced-->
  <string name="video_offline_header">Esse vídeo não está disponível off-line.</string>
  <!--Alert dialog content shown when user attempts to view a video that hasn't been synced-->
  <string name="video_offline_message">Selecione um vídeo que você baixou no seu dispositivo.</string>
  <!--New version available-->
  <!--Snackbar notification shown when the current version of the app is unsupported-->
  <string name="app_version_unsupported">A versão do app não é mais compatível.</string>
  <!--Snackbar notification shown when the current version of the app is deprecated-->
  <!--Snackbar notification shown when the current version of the app is outdated-->
  <string name="app_version_outdated">Uma nova versão do aplicativo está disponível.</string>
  <!--Snackbar action button label for opening an app store to upgrade the app to the latest version-->
  <string name="app_version_update_button">Atualizar</string>
  <!--Toast notification for when the user presses the update action button from the Snackbar
         notifications while no supported app store or browser is installed on the device with which
         the app could be updated-->
  <string name="app_version_upgrade_app_store_unavailable">Nenhuma loja de aplicativos compatível foi encontrada no dispositivo.</string>
  <!--Registration Error Messages-->
  <!--Error prompt shown during registration to indicate an empty field. Argument is a field name-->
  <string name="error_enter_field">Insira seu %1$s.</string>
  <!--Error prompt shown during registration to indicate a field missing a selection. Argument is a field name-->
  <string name="error_select_field">Selecione seu %1$s.</string>
  <!--Error prompt shown during registration when a field entry is too short. Arguments are 1) a field name, 2) a number-->
  <string name="error_min_length">%1$s deve ter ao menos %2$d caracteres.</string>
  <!--Error prompt shown during registration when a field entry is too long. Arguments are 1) a field name, 2) a number-->
  <string name="error_max_length">%1$s não deve ter mais de %2$d caracteres.</string>
  <!--Error message shown when a course hasn't started yet-->
  <string name="course_not_started">Este curso ainda não começou.</string>
  <!--Error message shown when a user can't perform an action on e.g. add a post/response/comment in forums-->
  <string name="action_not_completed">A ação não pôde ser completada.</string>
</resources>
