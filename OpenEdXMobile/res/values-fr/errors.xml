<?xml version='1.0' encoding='UTF-8'?>
<resources>
  <!--An error occurred but we don't know why-->
  <string name="error_unknown">Une erreur inconnu est survenu. Veuillez réessayer ultérieurement. </string>
  <!--Network error messages-->
  <!--Error shown when  user attempts an action but needs an internet connection-->
  <string name="network_not_connected">Vous n\'êtes pas connecté à Internet.</string>
  <!--Concise error shown when user attempts an action but needs an internet connection-->
  <string name="network_not_connected_short">Réseau non accessible</string>
  <!--Alert dialog title shown when a network request fails-->
  <string name="reset_no_network_title">Erreur de connexion</string>
  <!--Alert dialog content shown when a network request fails because the user has no internet connection-->
  <string name="reset_no_network_message">Vous n\'êtes pas connecté à Internet. Merci de vérifier votre connexion Internet.</string>
  <!--An error ocurred due to some kind of network failure-->
  <string name="network_connected_error">Une erreur de connexion est survenu. Veuillez vérifier votre connection internet.</string>
  <!--Title of prompt indicating issue loading a video-->
  <string name="network_error_header">Problème de chargement de la vidéo</string>
  <!--Content of prompt indicating issue loading a video-->
  <string name="network_error_message">Merci de vérifier votre connexion Internet.</string>
  <!--The server we are trying to use is not available right now-->
  <string name="network_service_unavailable">Service inaccessible. Veuillez réessayer ultérieurement.  </string>
  <!--Login error messages-->
  <!--Error shown during login when user attempts login without entering a username or e-mail-->
  <string name="error_enter_email">Veuillez entrez votre nom d\'utilisateur ou adresse électronique et essayer de nouveau.</string>
